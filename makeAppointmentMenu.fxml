<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="groupproject.MakeAppointmentMenuController">
   <children>
      <TextField fx:id="doctorsName" layoutX="52.0" layoutY="94.0" onAction="#doctorsName" />
      <Text layoutX="52.0" layoutY="80.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Doctor's Name:" />
      <TextField fx:id="appointmentNumber" layoutX="52.0" layoutY="163.0" onAction="#appointmentNumber" />
      <Text layoutX="52.0" layoutY="149.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Appointment #" />
      <TextArea fx:id="appointmentText" editable="false" layoutX="231.0" layoutY="19.0" prefHeight="370.0" prefWidth="356.0" />
      <Button fx:id="makeAppointment" layoutX="52.0" layoutY="309.0" mnemonicParsing="false" onAction="#makeAppointment" prefHeight="25.0" prefWidth="149.0" text="Make Appointment" />
      <Button fx:id="searchAppointment" layoutX="52.0" layoutY="269.0" mnemonicParsing="false" onAction="#searchAppointment" prefHeight="25.0" prefWidth="149.0" text="Search Appointments" />
      <TextField fx:id="patientName" layoutX="52.0" layoutY="226.0" onAction="#patientName" />
      <Text layoutX="52.0" layoutY="209.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Patient Name:" />
   </children>
</AnchorPane>
